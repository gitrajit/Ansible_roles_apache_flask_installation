<VirtualHost *:{{ SERVER_PORT }}>
		ServerName web.com
		ServerAdmin vagrant@wen.com
		WSGIScriptAlias / {{ path_flask }}/flaskapp.wsgi
		<Directory {{ path_flask }}>
			Order allow,deny
			Allow from all
		</Directory>
		Alias /static {{ path_flask }}/static
		<Directory {{ path_flask }}/static/>
			Order allow,deny
			Allow from all
		</Directory>
		ErrorLog ${APACHE_LOG_DIR}/error.log
		LogLevel warn
		CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
